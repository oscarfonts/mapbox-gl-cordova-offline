{
    "name": "mapbox-gl-cordova-mbtiles",
    "version": "0.0.6",
    "description": "A custom mapbox-gl build that reads vector tiles from a local mbtiles file in cordova",
    "main": "www/mapbox-gl-cordova-mbtiles.js",
    "scripts": {
        "version": "node node_modules/auto-changelog/lib/index.js -p && git add CHANGELOG.md",
        "start": "cordova run android & npm run watch & npm run server && fg",
        "watch": "watchify -s mapboxgl src/index.js -d -o www/mapbox-gl-cordova-mbtiles.js --verbose",
        "server": "live-server --watch=www --no-browser",
        "build": "browserify -s mapboxgl src/index.js -d --plugin [minifyify --map mapbox-gl-cordova-mbtiles.js.map --output www/mapbox-gl-cordova-mbtiles.js.map] -o www/mapbox-gl-cordova-mbtiles.js",
        "build-debug": "browserify -s mapboxgl src/index.js -d  -o www/mapbox-gl-cordova-mbtiles.js",
        "flow": "flow"
    },
    "keywords": [],
    "author": "Oscar Fonts <oscar.fonts@geomati.co>",
    "license": "ISC",
    "repository": {
        "type": "git",
        "url": "git@github.com:oscarfonts/mapbox-gl-cordova-mbtiles.git"
    },
    "devDependencies": {
        "@mapbox/batfish": "^0.13.3",
        "@mapbox/mapbox-gl-rtl-text": "^0.1.1",
        "auto-changelog": "1.4.0",
        "babel-eslint": "^7.0.0",
        "benchmark": "~2.1.0",
        "brfs": "^1.4.0",
        "browserify": "^14.0.0",
        "bubleify": "0.7.0",
        "coveralls": "^2.11.8",
        "d3": "^4.12.0",
        "derequire": "^2.0.6",
        "documentation": "5.3.3",
        "ejs": "^2.5.7",
        "envify": "^4.0.0",
        "eslint": "4.1.1",
        "eslint-config-mourner": "^2.0.0",
        "eslint-plugin-flowtype": "^2.34.0",
        "eslint-plugin-html": "^3.0.0",
        "eslint-plugin-node": "^5.1.1",
        "eslint-plugin-react": "^7.3.0",
        "execcommand-copy": "^1.1.0",
        "flow-bin": "^0.62.0",
        "flow-coverage-report": "^0.3.0",
        "flow-remove-types": "^1.0.4",
        "github-slugger": "^1.1.1",
        "gl": "^4.0.1",
        "glob": "^7.0.3",
        "in-publish": "^2.0.0",
        "is-builtin-module": "^1.0.0",
        "jsdom": "^11.2.0",
        "json-stringify-pretty-compact": "^1.0.4",
        "live-server": "1.2.0",
        "lodash": "^4.16.0",
        "mapbox-gl": "github:JpmGuides/mapbox-gl-js#v0.43.0-localfiles",
        "minifyify": "^7.0.1",
        "mock-geolocation": "^1.0.11",
        "npm-run-all": "^4.0.1",
        "nyc": "^10.1.2",
        "object.entries": "^1.0.4",
        "pako": "1.0.5",
        "pngjs": "^3.0.0",
        "prismjs": "^1.8.1",
        "prop-types": "^15.6.0",
        "proxyquire": "^1.7.9",
        "raw-loader": "^0.5.1",
        "react": "^16.0.0",
        "react-dom": "^16.0.0",
        "react-helmet": "^5.2.0",
        "remark": "^8.0.0",
        "remark-html": "^5.0.1",
        "remark-react": "^4.0.1",
        "request": "^2.79.0",
        "sinon": "^2.1.0",
        "slugg": "^1.2.1",
        "st": "^1.2.0",
        "stylelint": "^7.10.1",
        "tap": "^10.3.0",
        "unassertify": "^2.0.0",
        "unflowify": "^1.0.0",
        "watchify": "^3.9.0",
        "webworkify": "1.4.0"
    },
    "browserify": {
        "transform": [
            "unflowify",
            [
                "bubleify",
                {
                    "bubleError": true,
                    "transforms": {
                        "dangerousForOf": true
                    },
                    "objectAssign": "Object.assign"
                }
            ],
            "package-json-versionify",
            "unassertify",
            "brfs"
        ]
    },
    "dependencies": {
        "@mapbox/gl-matrix": "^0.0.1",
        "@mapbox/mapbox-gl-supported": "^1.3.0",
        "@mapbox/point-geometry": "^0.1.0",
        "@mapbox/shelf-pack": "^3.1.0",
        "@mapbox/tiny-sdf": "^1.1.0",
        "@mapbox/unitbezier": "^0.0.0",
        "@mapbox/vector-tile": "^1.3.0",
        "@mapbox/whoots-js": "^3.0.0",
        "base64-js": "1.2.1",
        "brfs": "^1.4.0",
        "cordova-android": "6.2.3",
        "cordova-ios": "4.5.4",
        "cordova-plugin-compat": "1.2.0",
        "cordova-plugin-device": "1.1.6",
        "cordova-plugin-file": "4.3.3",
        "cordova-plugin-whitelist": "1.3.2",
        "cordova-sqlite-ext": "0.10.5",
        "csscolorparser": "~1.0.2",
        "earcut": "^2.1.3",
        "geojson-rewind": "^0.3.0",
        "geojson-vt": "^3.0.0",
        "gray-matter": "^3.0.8",
        "grid-index": "^1.0.0",
        "jsonlint-lines-primitives": "~1.6.0",
        "minimist": "0.0.8",
        "package-json-versionify": "^1.0.2",
        "pbf": "^3.0.5",
        "quickselect": "^1.0.0",
        "rw": "^1.3.3",
        "shuffle-seed": "^1.1.6",
        "sort-object": "^0.3.2",
        "supercluster": "^2.3.0",
        "through2": "^2.0.3",
        "tinyqueue": "^1.1.0",
        "vt-pbf": "^3.0.1"
    },
    "cordova": {
        "plugins": {
            "cordova-plugin-whitelist": {},
            "cordova-plugin-file": {},
            "cordova-sqlite-ext": {},
            "cordova-plugin-device": {}
        },
        "platforms": [
            "android",
            "ios"
        ]
    }
}
